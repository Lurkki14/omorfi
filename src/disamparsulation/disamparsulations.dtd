<!ELEMENT disamparsulations (evidence*)>
<!ATTLIST disamparsulations
    version CDATA #REQUIRED
    xmlns:xsi CDATA #IMPLIED
    xsi:noNamespaceSchemaLocation CDATA #IMPLIED>
<!ELEMENT evidence (target, likelihood, depname?, context?)>
<!ATTLIST evidence
    name CDATA #REQUIRED>
<!ELEMENT target (upos?, ufeats?)>
<!ELEMENT upos (#PCDATA)>
<!ELEMENT ufeats (ufeat*)>
<!ELEMENT ufeat (#PCDATA)>
<!ATTLIST ufeat
    name CDATA #REQUIRED>
<!ELEMENT likelihood (#PCDATA)>
<!ELEMENT depname (#PCDATA)>
<!ELEMENT context (location, upos?, ufeats?, barrier?)>
<!ELEMENT location (#PCDATA)>
<!ELEMENT barrier (upos?, ufeats?)>

