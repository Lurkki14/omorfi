<disamparsulations version="0.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="disamparsulations.xsd"
    xmlns:html="http://www.w3.org/1999/xhtml">
  <evidences>
    <!-- {{{ Single word stuffs -->
    <evidence name="Styles unlikely">
      <documentation>
        <example>myös hän oli siellä</example>
      </documentation>
      <target>
        <match>
          <ufeats>
            <ufeat name="Style">Coll</ufeat>
          </ufeats>
        </match>
      </target>
      <likelihood>unlikely</likelihood>
    </evidence>
    <evidence name="Typos unlikely">
      <target>
        <match>
          <ufeats>
            <ufeat name="Typo">Yes</ufeat>
          </ufeats>
        </match>
      </target>
      <likelihood>unlikely</likelihood>
    </evidence>
    <evidence name="instructives unlikely">
      <documentation>
        <example>hän oli ryhmänsä nopein</example>
      </documentation>
      <target>
        <match>
          <ufeats>
            <ufeat name="Case">Ins</ufeat>
          </ufeats>
        </match>
      </target>
      <likelihood>unlikely</likelihood>
    </evidence>
    <evidence name="rather rare nouns that shadow common function words">
      <documentation>
        <example>vain elämää</example>
      </documentation>
      <target>
        <match>
          <lemma>edus</lemma>
          <lemma>kos</lemma>
          <lemma>kui</lemma>
          <lemma>kuvi</lemma>
          <lemma>lev</lemma>
          <lemma>mon</lemma>
          <lemma>pai</lemma>
          <lemma>par</lemma>
          <lemma>tai</lemma>
          <lemma>taka</lemma>
          <lemma>tule</lemma>
          <lemma>vai</lemma>
          <upos>NOUN</upos>
        </match>
      </target>
      <likelihood>unlikely</likelihood>
    </evidence>
    <evidence name="rather rare proper nouns that shadow common nouns etc.">
      <documentation>
        <example>Vanha Mies ja Pyhä Metsä</example>
      </documentation>
      <target>
        <match>
          <lemma>Vanha</lemma>
          <lemma>Mei</lemma>
          <lemma>Osa</lemma>
          <lemma>Hanska</lemma>
          <lemma>Osta</lemma>
          <lemma>Leikkaa</lemma>
          <lemma>Varo</lemma>
          <lemma>Tarkki</lemma>
          <lemma>Onkki</lemma>
          <lemma>Ede</lemma>
          <lemma>Mietti</lemma>
          <lemma>Metsä</lemma>
          <upos>PROPN</upos>
        </match>
      </target>
      <likelihood>unlikely</likelihood>
    </evidence>
    <evidence name="rare adverbs that have commoner adposition or noun versions">
      <documentation>
        <example>viisi pitkää tikkua</example>
      </documentation>
      <target>
        <match>
          <lemma>kuin</lemma>
          <lemma>kun</lemma>
          <lemma>tai</lemma>
          <lemma>sen</lemma>
          <lemma>viisi</lemma>
          <lemma>kuuluu</lemma>
          <lemma>koska</lemma>
          <lemma>pitkää</lemma>
          <lemma>sinut</lemma>
          <upos>ADV</upos>
        </match>
      </target>
      <likelihood>unlikely</likelihood>
    </evidence>
    <evidence name="rare verbs that partially overlap nouns">
      <documentation>
        <example>juuri nytkin siitä mennään</example>
      </documentation>
      <target>
        <match>
          <lemma>juuria</lemma>
          <lemma>eritä</lemma>
          <lemma>niittää</lemma>
          <lemma>nytkiä</lemma>
          <lemma>siitä</lemma>
          <lemma>siittää</lemma>
          <lemma>suomia</lemma>
          <lemma>suoria</lemma>
          <lemma>vastoa</lemma>
          <lemma>oieta</lemma>
          <lemma>niistää</lemma>
          <upos>VERB</upos>
        </match>
      </target>
      <likelihood>uncommonly</likelihood>
    </evidence>
    <evidence name="typically adverbs though sometimes noun etc. reading is ok">
      <documentation>
        <example>läpeensä paha</example>
      </documentation>
      <target>
        <match>
          <lemma>erikseen</lemma>
          <lemma>erilleen</lemma>
          <lemma>erillään</lemma>
          <lemma>lainkaan</lemma>
          <lemma>läpeensä</lemma>
          <lemma>kaikkiaan</lemma>
          <lemma>kumminkaan</lemma>
          <lemma>kumminkin</lemma>
          <lemma>kotoisin</lemma>
          <upos>ADV</upos>
        </match>
      </target>
      <likelihood>unlikely</likelihood>
    </evidence>
    <evidence name="typically adverbs though sometimes noun etc. reading is ok">
      <documentation>
        <example>käydään yhdessä sotaan</example>
      </documentation>
      <target>
        <match>
          <lemma>yhdessä</lemma>
          <lemma>vinossa</lemma>
          <lemma>pystyssä</lemma>
          <upos>ADV</upos>
        </match>
      </target>
      <likelihood>probably</likelihood>
      <context negated="yes">
        <location>+1</location>
        <match>
          <ufeats>
            <ufeat name="Case">Ine</ufeat>
          </ufeats>
        </match>
      </context>
    </evidence>
    <evidence name="Second singular possessives unlikely unless proven">
      <documentation>
        <example>voisi olla</example>
      </documentation>
      <target>
        <match>
          <lemma>kuu</lemma>
          <lemma>voi</lemma>
          <lemma>tie</lemma>
          <lemma>tunti</lemma>
          <lemma>lupa</lemma>
          <lemma>rupi</lemma>
          <lemma>hai</lemma>
          <lemma>taju</lemma>
          <lemma>pyy</lemma>
          <lemma>kerä</lemma>
          <lemma>vasta</lemma>
          <lemma>pää</lemma>
          <lemma>harja</lemma>
          <lemma>vuo</lemma>
          <lemma>halu</lemma>
          <lemma>tapa</lemma>
          <lemma>tosi</lemma>
          <lemma>pola</lemma>
          <lemma>poli</lemma>
          <upos>NOUN</upos>
          <ufeats>
            <ufeat name="Number[psor]">Sing</ufeat>
            <ufeat name="Person[psor]">2</ufeat>
          </ufeats>
        </match>
      </target>
      <likelihood>unlikely</likelihood>
      <context negated="yes">
        <location>-1</location>
        <match>
          <upos>PRON</upos>
          <ufeats>
            <ufeat name="PronType">Prs</ufeat>
            <ufeat name="Case">Gen</ufeat>
            <ufeat name="Number">Sing</ufeat>
            <ufeat name="Person">2</ufeat>
          </ufeats>
        </match>
      </context>
    </evidence>
    <evidence name="sekä is probably not cconj without että">
      <target>
        <match>
          <lemma>sekä</lemma>
          <upos>CCONJ</upos>
        </match>
      </target>
      <likelihood>unlikely</likelihood>
      <context negated="yes">
        <location>right</location>
        <match>
          <lemma>että</lemma>
        </match>
      </context>
    </evidence>
    <!-- }}}
         {{{ deps -->
    <evidence name="puncts to nearest main verb">
      <target>
        <match>
          <upos>PUNCT</upos>
        </match>
      </target>
      <likelihood>usually</likelihood>
      <depname>punct</depname>
      <context>
        <location>any</location>
        <match>
          <upos>VERB</upos>
        </match>
        <barrier>
          <match>
            <upos>PUNCT</upos>
          </match>
        </barrier>
      </context>
    </evidence>
    <evidence name="finite verbs can be roots">
      <target>
        <match>
          <upos>VERB</upos>
          <ufeats>
            <ufeat name="VerbForm">Fin</ufeat>
          </ufeats>
        </match>
      </target>
      <likelihood>probably</likelihood>
      <depname>root</depname>
      <context>
        <location>ROOT</location>
      </context>
    </evidence>
    <evidence name="cconj coordinates its +1">
      <target>
        <match>
          <upos>CCONJ</upos>
        </match>
      </target>
      <likelihood>probably</likelihood>
      <depname>conj</depname>
      <context>
        <location>+1</location>
      </context>
    </evidence>
    <evidence name="elative adj agrees its +1">
      <target>
        <match>
          <upos>ADJ</upos>
          <ufeats>
            <ufeat name="Case">Ela</ufeat>
          </ufeats>
        </match>
      </target>
      <likelihood>usually</likelihood>
      <depname>amod</depname>
      <context>
        <location>+1</location>
        <match>
          <ufeats>
            <ufeat name="Case">Ela</ufeat>
          </ufeats>
        </match>
      </context>
    </evidence>
    <evidence name="1sg pronoung agrees with 1sg verb">
      <target>
        <match>
          <upos>PRON</upos>
          <ufeats>
            <ufeat name="PronType">Prs</ufeat>
            <ufeat name="Number">Sing</ufeat>
            <ufeat name="Person">1</ufeat>
          </ufeats>
        </match>
      </target>
      <likelihood>usually</likelihood>
      <depname>nsubj</depname>
      <context>
        <location>any</location>
        <match>
          <upos>VERB</upos>
          <ufeats>
            <ufeat name="Number">Sing</ufeat>
            <ufeat name="Person">1</ufeat>
          </ufeats>
        </match>
        <barrier>
          <match>
            <upos>VERB</upos>
          </match>
        </barrier>
      </context>
    </evidence>
    <evidence name="Numeral modifier for nom takes par">
      <target>
        <match>
          <upos>NUM</upos>
          <ufeats>
            <ufeat name="NumType">Card</ufeat>
            <ufeat name="Number">Sing</ufeat>
            <ufeat name="Case">Nom</ufeat>
          </ufeats>
        </match>
      </target>
      <likelihood>usually</likelihood>
      <depname>nummod</depname>
      <context>
        <location>+1</location>
        <match>
          <upos>NOUN</upos>
        <ufeats>
          <ufeat name="Number">Sing</ufeat>
          <ufeat name="Case">Par</ufeat>
        </ufeats>
      </match>
      </context>
    </evidence>
    <evidence name="Partitives can be objects">
      <target>
        <match>
          <upos>NOUN</upos>
          <ufeats>
            <ufeat name="Case">Par</ufeat>
          </ufeats>
        </match>
      </target>
      <likelihood>possibly</likelihood>
      <depname>obj</depname>
      <context>
        <location>any</location>
        <match>
          <upos>VERB</upos>
        </match>
        <barrier>
          <match>
            <upos>PUNCT</upos>
          </match>
        </barrier>
      </context>
    </evidence>
    <evidence name="Adverbs are ad verbs">
      <target>
        <match>
          <upos>ADV</upos>
        </match>
      </target>
      <likelihood>possibly</likelihood>
      <depname>advmod</depname>
      <context>
        <location>any</location>
        <match>
          <upos>VERB</upos>
        </match>
        <barrier>
          <match>
            <upos>PUNCT</upos>
          </match>
        </barrier>
      </context>
    </evidence>
    <evidence name="Genitive prn to possessive">
      <target>
        <match>
          <upos>PRON</upos>
          <ufeats>
            <ufeat name="PronType">Prs</ufeat>
            <ufeat name="Case">Gen</ufeat>
            <ufeat name="Number">Sing</ufeat>
            <ufeat name="Person">2</ufeat>
          </ufeats>
        </match>
      </target>
      <likelihood>usually</likelihood>
      <depname>nmod:poss</depname>
      <context>
        <location>right</location>
        <match>
          <ufeats>
            <ufeat name="Number[psor]">Sing</ufeat>
            <ufeat name="Person[psor]">2</ufeat>
          </ufeats>
        </match>
        <barrier>
          <match>
            <upos>PUNCT</upos>
          </match>
        </barrier>
      </context>
    </evidence>
    <evidence name="Adessive before copula for possession">
      <target>
        <match>
          <ufeats>
            <ufeat name="Case">Ade</ufeat>
          </ufeats>
        </match>
      </target>
      <likelihood>possibly</likelihood>
      <depname>nmod:own</depname>
      <context>
        <location>+1</location>
        <match>
          <lemma>olla</lemma>
          <upos>AUX</upos>
        </match>
      </context>
    </evidence>
    <evidence name="Genitive could be a possession">
      <target>
        <match>
          <ufeats>
            <ufeat name="Case">Gen</ufeat>
          </ufeats>
        </match>
      </target>
      <likelihood>possibly</likelihood>
      <depname>nmod:poss</depname>
      <context>
        <location>+1</location>
        <match>
          <upos>NOUN</upos>
        </match>
      </context>
    </evidence>
    <evidence name="Postpositions with genitive complement can be case">
      <target>
        <match>
          <lemma>takana</lemma>
          <lemma>mukaan</lemma>
          <lemma>vuoksi</lemma>
          <lemma>alla</lemma>
          <lemma>kanssa</lemma>
          <lemma>kautta</lemma>
          <lemma>puolesta</lemma>
          <lemma>läpi</lemma>
          <lemma>johdolla</lemma>
          <lemma>sisällä</lemma>
          <lemma>perusteella</lemma>
          <lemma>sijaan</lemma>
          <lemma>suhteen</lemma>
          <lemma>avulla</lemma>
          <lemma>jälkeen</lemma>
          <lemma>myötä</lemma>
          <lemma>edessä</lemma>
          <lemma>alle</lemma>
          <upos>ADP</upos>
          <ufeats>
            <ufeat name="AdpType">Post</ufeat>
          </ufeats>
        </match>
      </target>
      <likelihood>probably</likelihood>
      <depname>case</depname>
      <context>
        <location>left</location>
        <match>
          <ufeats>
            <ufeat name="Case">Gen</ufeat>
          </ufeats>
        </match>
        <barrier>
          <match>
            <upos>PUNCT</upos>
          </match>
        </barrier>
      </context>
    </evidence>
    <evidence name="Postpositions with partitive complement can be case">
      <target>
        <match>
          <lemma>ylös</lemma>
          <lemma>päin</lemma>
          <lemma>vastaan</lemma>
          <lemma>ilman</lemma>
          <lemma>kautta</lemma>
          <lemma>huolimatta</lemma>
          <lemma>ennen</lemma>
          <upos>ADP</upos>
          <ufeats>
            <ufeat name="AdpType">Post</ufeat>
          </ufeats>
        </match>
      </target>
      <likelihood>probably</likelihood>
      <depname>case</depname>
      <context>
        <location>left</location>
        <match>
          <ufeats>
            <ufeat name="Case">Par</ufeat>
          </ufeats>
        </match>
        <barrier>
          <match>
            <upos>PUNCT</upos>
          </match>
        </barrier>
      </context>
    </evidence>
    <!-- agreements (I'll maybe make macro agreement later) -->
    <evidence name="Adjective agreeing in nominative">
      <target>
        <match>
          <upos>ADJ</upos>
          <ufeats>
            <ufeat name="Case">Nom</ufeat>
          </ufeats>
        </match>
      </target>
      <likelihood>usually</likelihood>
      <depname>amod</depname>
      <context>
        <location>+1</location>
        <match>
          <upos>NOUN</upos>
          <ufeats>
            <ufeat name="Case">Nom</ufeat>
          </ufeats>
        </match>
      </context>
    </evidence>
    <evidence name="Adjective agreeing in inessive">
      <target>
        <match>
          <upos>ADJ</upos>
          <ufeats>
            <ufeat name="Case">Ine</ufeat>
          </ufeats>
        </match>
      </target>
      <likelihood>usually</likelihood>
      <depname>amod</depname>
      <context>
        <location>+1</location>
        <match>
          <upos>NOUN</upos>
          <ufeats>
            <ufeat name="Case">Ine</ufeat>
          </ufeats>
        </match>
      </context>
    </evidence>
    <evidence name="Adjective agreeing in illative">
      <target>
        <match>
          <upos>ADJ</upos>
          <ufeats>
            <ufeat name="Case">Ill</ufeat>
          </ufeats>
        </match>
      </target>
      <likelihood>usually</likelihood>
      <depname>amod</depname>
      <context>
        <location>+1</location>
        <match>
          <upos>NOUN</upos>
          <ufeats>
            <ufeat name="Case">Ill</ufeat>
          </ufeats>
        </match>
      </context>
    </evidence>
    <evidence name="Adjective agreeing in illative">
      <target>
        <match>
          <upos>ADJ</upos>
          <ufeats>
            <ufeat name="Case">Par</ufeat>
          </ufeats>
        </match>
      </target>
      <likelihood>usually</likelihood>
      <depname>amod</depname>
      <context>
        <location>+1</location>
        <match>
          <upos>NOUN</upos>
          <ufeats>
            <ufeat name="Case">Par</ufeat>
          </ufeats>
        </match>
      </context>
    </evidence>
    <evidence name="Adjective agreeing in allative">
      <target>
        <match>
          <upos>ADJ</upos>
          <ufeats>
            <ufeat name="Case">All</ufeat>
          </ufeats>
        </match>
      </target>
      <likelihood>usually</likelihood>
      <depname>amod</depname>
      <context>
        <location>+1</location>
        <match>
          <upos>NOUN</upos>
          <ufeats>
            <ufeat name="Case">All</ufeat>
          </ufeats>
        </match>
      </context>
    </evidence>
    <evidence name="Determiner agreement in inessive">
      <target>
        <match>
          <lemma>se</lemma>
          <lemma>kaikki</lemma>
          <lemma>tämä</lemma>
          <upos>PRON</upos>
          <upos>DET</upos>
          <ufeats>
            <ufeat name="Case">Ine</ufeat>
          </ufeats>
        </match>
      </target>
      <likelihood>usually</likelihood>
      <depname>det</depname>
      <context>
        <location>+1</location>
        <match>
          <upos>NOUN</upos>
        </match>
      </context>
    </evidence>

  </evidences>
</disamparsulations>
