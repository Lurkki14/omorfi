  <evidences>
    <!-- {{{ Single word stuffs -->
    <!-- {{{ rare tags extra penalties -->
    <evidence name="Styles unlikely">
      <documentation>
        <example>myös hän oli siellä</example>
      </documentation>
      <target>
        <match>
          <ufeats>
            <ufeat name="Style">Coll</ufeat>
          </ufeats>
        </match>
      </target>
      <likelihood>unlikely</likelihood>
    </evidence>
    <evidence name="Typos unlikely">
      <target>
        <match>
